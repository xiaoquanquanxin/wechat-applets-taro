{
    "extends": [
        "taro/react"
    ],
    "rules": {
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off",
        "react/jsx-tag-spacing": "off",
        "react/jsx-indent-props": "off",
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": [
            "off"
        ],
        "import/first": "off"
    }
}
